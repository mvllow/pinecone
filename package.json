{
	"name": "pinecone-cli",
	"version": "1.6.2",
	"description": "Lovely VSCode theme builder",
	"license": "MIT",
	"main": "dist/index.js",
	"repository": "mvllow/pinecone",
	"bin": {
		"pinecone": "dist/cli.js",
		"pinecone-cli": "dist/cli.js"
	},
	"scripts": {
		"dev": "ts-node source/cli.ts",
		"clean": "del-cli dist themes pinecone.config.js",
		"build": "npm run clean && tsc && npm test",
		"test": "ava"
	},
	"files": [
		"dist"
	],
	"types": "dist/types/index.d.ts",
	"keywords": [
		"dynamic",
		"vscode",
		"themes",
		"json",
		"variables"
	],
	"dependencies": {
		"chalk": "^4.1.1",
		"meow": "^9.0.0",
		"node-watch": "^0.7.1",
		"prettier": "^2.3.1",
		"slugify": "^1.5.3"
	},
	"devDependencies": {
		"@types/node": "^15.12.2",
		"@types/prettier": "^2.3.0",
		"@types/sinon": "^10.0.2",
		"ava": "^3.15.0",
		"del-cli": "^3.0.1",
		"sinon": "^11.1.1",
		"ts-node": "^10.0.0",
		"typescript": "^4.3.2"
	},
	"prettier": {
		"printWidth": 100,
		"useTabs": true,
		"semi": false,
		"singleQuote": true
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	}
}
