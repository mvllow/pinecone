{
  "name": "pinecone-cli",
  "version": "1.2.1",
  "description": "Lovely VSCode theme builder",
  "license": "MIT",
  "repository": "mvllow/pinecone",
  "bin": {
    "pinecone": "dist/cli.js",
    "pinecone-cli": "dist/cli.js"
  },
  "scripts": {
    "dev": "ts-node source/cli.ts",
    "clean": "del-cli dist themes pinecone.config.js",
    "build": "npm run clean && tsc && npm test",
    "test": "ava"
  },
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "keywords": [
    "dynamic",
    "vscode",
    "themes",
    "json",
    "variables"
  ],
  "dependencies": {
    "chalk": "^4.1.0",
    "meow": "^8.1.0",
    "prettier": "^2.2.1",
    "slugify": "^1.4.6"
  },
  "devDependencies": {
    "@types/node": "^14.14.19",
    "@types/prettier": "^2.1.6",
    "@types/sinon": "^9.0.10",
    "ava": "^3.15.0",
    "del-cli": "^3.0.1",
    "sinon": "^9.2.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
