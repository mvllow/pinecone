{
  "name": "@mvllow/pinecone",
  "version": "0.4.0-beta.1",
  "description": "Dynamic vscode theme generator",
  "author": "mvllow <mvllow@icloud.com>",
  "license": "MIT",
  "repository": "https://github.com/mvllow/pinecone.git",
  "bugs": "https://github.com/mvllow/pinecone/issues",
  "homepage": "https://github.com/mvllow/pinecone#readme",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "pinecone": "dist/index.js"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "watch": "npm run build:types && npm run build:js -- --watch",
    "prepare": "npm run build",
    "publish:public": "npm publish --access public",
    "publish:beta": "npm publish --tag beta"
  },
  "devDependencies": {
    "jest": "^26.4.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  }
}
